{
	"name": "fliglio/borg-demo",
	"description": "Demo Application for Fliglio Framework",
	"type": "project",
	"license": "GPL-3.0+",
	"authors": [{
		"name": "Ben Schwartz",
		"email": "benschw@gmail.com"
	}],
	"require": {
		"php": ">=5.3",
		"fliglio/fli":     "dev-master",
		"fliglio/flfc":    "dev-master",
		"fliglio/routing": "dev-master",
		"fliglio/http":    "dev-master",
		"fliglio/web":     "dev-master",
		"symfony/validator": "2.*",
		"doctrine/annotations": "1.*",
		"doctrine/cache": "1.*",
		"guzzlehttp/guzzle": "~6.0",
		"robmorgan/phinx": "^0.5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1.0"
	},
	"minimum-stability" : "dev",
	"autoload": {
		"psr-0": {
			"Demo\\": "src/main"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"Demo\\" : "src/test"
		}
	}
}
