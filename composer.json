{
	"name": "fliglio/rest-gs",
	"description": "Demo Application for Fliglio Framework",
	"type": "project",
	"license": "GPL-3.0+",
	"authors": [{
		"name": "Ben Schwartz",
		"email": "benschw@gmail.com"
	}],
	"require": {
		"php": ">=5.6",
		"fliglio/fli":          "dev-master",
		"fliglio/flfc":         "dev-master",
		"fliglio/routing":      "dev-master",
		"fliglio/http":         "dev-master",
		"fliglio/web":          "dev-master",
		"fliglio/logging":      "dev-master",
		"doctrine/annotations": "1.*",
		"doctrine/cache":       "1.*",
		"guzzlehttp/guzzle":    "~5.0",
		"robmorgan/phinx":      "^0.5.0",
		"monolog/monolog":      "1.*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.5.*"
	},
	"autoload": {
		"psr-4": {
			"Demo\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Demo\\" : "test/"
		}
